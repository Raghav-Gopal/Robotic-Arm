<robot name="Manipulator">
<link name="base_link"/>
<link name="link1"/>
<link name="link2"/>
<link name="link3"/>
<link name="end effector"/>

<joint name="Revolute 1" type="revolute">
<parent link="base_link"/>
<child link="link1"/>
<origin xyz="0 0 0"/>
<axis xyz="0 0 1"/>
</joint>

<joint name="Revolute 2" type="revolute">
<parent link="link1"/>
<child link="link2"/>
<origin xyz="0 0 10"/>
<axis xyz="1 0 0"/>
</joint>

<joint name="Revolute 3" type="revolute">
<parent link="link2"/>
<child link="link3"/>
<origin xyz="0 0 160"/>
<axis xyz="0 1 0"/>
</joint>

<joint name="Fixed End Effector" type="fixed">
<parent link="link3"/>
<child link="end_effector"/>
<origin xyz="0 0 -92.5"/>
</joint>
</robot>